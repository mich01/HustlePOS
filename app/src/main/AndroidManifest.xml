<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="hustlepos">

<uses-feature
    android:name="android.hardware.camera"
    android:required="true" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USE_BIOMETRIC" />
<queries>
    <intent>
        <action android:name="android.media.action.IMAGE_CAPTURE" />
    </intent>
</queries>
<application
    android:allowBackup="false"
    android:fullBackupContent="false"
    android:icon="@drawable/logo"
    android:label="@string/app_name"
    android:requestLegacyExternalStorage="true"
    android:usesCleartextTraffic="false"
    android:roundIcon="@drawable/logo"
    android:supportsRtl="true"
    android:theme="@style/Theme.MyBiz">
    <activity
        android:name="com.mich01.hustlepos.UI.Setup.BackupRestore"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.Analytics.Analytics"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.Sales.Search_Records"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.Support.SupportActivity"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.Sales.ViewSales"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.Sales.ViewTransactions"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.Sales.Transaction_Checkout"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.Sales.Transactions"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.Items.ItemList"
        android:exported="true"
        android:permission="android.permission.READ_EXTERNAL_STORAGE"/>
    <activity
        android:name="com.mich01.hustlepos.UI.Setup.RegisterActivity"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.LoginActivity"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.UI.HomeActivity"
        android:exported="false" />
    <activity
        android:name="com.mich01.hustlepos.SplashActivity"
        android:exported="true">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
</application>
</manifest>
